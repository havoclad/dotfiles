Include peak_config

ServerAliveInterval 120
ServerAliveCountMax 10000

Host *
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa
  AddKeysToAgent yes
  SendEnv COLORTERM

Host pair
  HostName havoc.pairserver.com
  User havoc
  ForwardAgent yes

Host 192.168.99.1
  User pat
Host router.pat
  User pat

